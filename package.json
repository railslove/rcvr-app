  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node ./scripts/build.js",
    "next-build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "typecheck": "tsc --noemit",
